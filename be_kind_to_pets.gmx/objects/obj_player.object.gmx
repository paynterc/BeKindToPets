            <kind>1</kind>
            <string>///React to inputs
move = key_left + key_right;
hsp = move * movespeed;
var jumping = false;

if(can_jump)
{
    if (key_jump){
        vsp = -jumpspeed;
        jumping = true;
    }
}

can_jump = false;
hsp_final = hsp + hsp_carry;
hsp_carry = 0;
var hit_player = noone;
var grav_temp = grav;


//Horizontal Collision Rope
if (instance_exists(obj_rope))
{
    hit_rope = collision_point(x,y,obj_rope,false,true);
    
    if(hit_rope){
        grav_temp = 0;
        vsp = -3;
    }
}

if(vsp&lt;10)
{
    vsp += grav_temp;
}


//Standing on Player
hit_player = collision_point(x,y+1,obj_player,false,true);
if(hit_player &amp;&amp; !jumping)
{
    vsp = (hit_player).vsp;
    hsp_carry = (hit_player).hsp_final;
    can_jump = true;
}
else if(collision_point(x,y+vsp,obj_player,false,true))
{
    //Landing on Player
    while(!collision_point(x,y+sign(vsp),obj_player,false,true))
    {
        y += sign(vsp);
    }
    vsp=0;
    can_jump = true;
}



//Moving Platform
//Remember, y axis increases as you go down.
if (instance_exists(obj_platform_mv))
{
 
    if (place_meeting(x,y+vsp,obj_platform_mv))
    {
    
        while(!place_meeting(x,y+sign(vsp),obj_platform_mv))
        {
            y += sign(vsp);
        }
    }
    
    var hit_platform_mv = collision_point(x,y+1,obj_platform_mv,false,true);
    if(hit_platform_mv)
    {
        vsp = 0;
        hsp_carry = (hit_platform_mv).hsp;
        can_jump = true;
    }
    
}


//Horizontal Collision Check
if (place_meeting(x+hsp_final,y,obj_wall))
{
    while(!place_meeting(x+sign(hsp_final),y,obj_wall))
    {
        x += sign(hsp_final);
    }
    hsp_final = 0;
    hsp = 0;
}
x += hsp_final;

//Vertical Collision Check
if (place_meeting(x,y+vsp,obj_wall))
{
    while(!place_meeting(x,y+sign(vsp),obj_wall))
    {
        y += sign(vsp);
    }
    
    vsp = 0;
    can_jump = true;
    
    if(hit_player)
    {
        hit_player.vsp=0;
    }
}
y += vsp;
</string>
